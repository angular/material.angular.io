{
  "name": "material-angular-io",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "yarn build-themes && ng serve",
    "start:jit": "yarn build-themes && ng serve --aot=false",
    "start:prod": "yarn build-themes && ng serve --prod",
    "lint": "tslint -p tsconfig.json",
    "test": "yarn build-themes && ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "yarn build-themes && ng e2e",
    "build": "yarn build-themes && ng build",
    "build:content": "yarn upgrade @angular/material-examples",
    "build:highmem": "yarn build-themes && node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build",
    "build:sm": "ng build --prod --source-map",
    "build-themes": "bash ./tools/build-themes.sh",
    "prod-build": "yarn build-themes && ng build --prod",
    "postinstall": "webdriver-manager update --gecko false && yarn build-themes",
    "publish-prod": "bash ./tools/deploy.sh stable prod",
    "publish-dev": "bash ./tools/deploy.sh",
    "publish-beta": "bash ./tools/deploy.sh stable beta"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0-rc.0",
    "@angular/cdk": "^9.0.0-rc.0",
    "@angular/cdk-experimental": "^9.0.0-rc.0",
    "@angular/common": "^9.0.0-rc.0",
    "@angular/compiler": "^9.0.0-rc.0",
    "@angular/core": "^9.0.0-rc.0",
    "@angular/forms": "^9.0.0-rc.0",
    "@angular/material": "^9.0.0-rc.0",
    "@angular/material-examples": "angular/material2-docs-content",
    "@angular/material-experimental": "^9.0.0-rc.0",
    "@angular/material-moment-adapter": "^9.0.0-rc.0",
    "@angular/platform-browser": "^9.0.0-rc.0",
    "@angular/platform-browser-dynamic": "^9.0.0-rc.0",
    "@angular/router": "^9.0.0-rc.0",
    "core-js": "^2.6.9",
    "hammerjs": "^2.0.8",
    "material-components-web": "^4.0.0-alpha.0",
    "moment": "^2.24.0",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.0",
    "@angular/cli": "^9.0.0-rc.0",
    "@angular/compiler-cli": "^9.0.0-rc.0",
    "@angular/localize": "^9.0.0-rc.0",
    "@types/jasmine": "^3.4.5",
    "@types/node": "^12.12.5",
    "firebase-tools": "^7.6.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "node-sass": "^4.13.0",
    "protractor": "^5.4.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "3.6.4"
  }
}
