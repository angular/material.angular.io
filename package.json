{
  "name": "material-angular-io",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "ng build --prod",
    "build-themes": "bash ./tools/build-themes.sh",
    "e2e": "protractor",
    "fetch-local": "bash ./tools/fetch-assets-local.sh",
    "lint": "tslint \"src/**/*.ts\"",
    "ng": "ng",
    "pree2e": "webdriver-manager update",
    "prod-build": "npm run build-themes && ng build --aot --prod",
    "postinstall": "webdriver-manager update && bash ./tools/fetch-assets.sh",
    "publish-dev": "npm run build-themes && ng build --aot --prod && firebase use material2-docs-dev && firebase deploy",
    "publish-prod": "npm run build-themes && ng build --aot --prod && firebase use material-angular-io && firebase deploy",
    "start": "npm run build-themes && ng serve --aot --sourcemaps=false",
    "start-jit": "npm run build-themes && ng serve --sourcemaps=false",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.1",
    "@angular/cdk": "^5.2.1",
    "@angular/common": "^5.2.1",
    "@angular/compiler": "^5.2.1",
    "@angular/core": "^5.2.1",
    "@angular/forms": "^5.2.1",
    "@angular/http": "^5.2.1",
    "@angular/material": "^5.2.1",
    "@angular/material-moment-adapter": "^5.2.1",
    "@angular/platform-browser": "^5.2.1",
    "@angular/platform-browser-dynamic": "^5.2.1",
    "@angular/router": "^5.2.1",
    "@angular/service-worker": "^5.2.1",
    "core-js": "^2.4.1",
    "moment": "^2.19.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.2",
    "@angular/compiler-cli": "^5.2.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^7.0.5",
    "codelyzer": "^4.0.1",
    "firebase-tools": "^3.15.3",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.9.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-browserstack-launcher": "^1.2.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.2.1",
    "karma-sauce-launcher": "^1.1.0",
    "node-sass": "^4.5.3",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.4.0"
  }
}
