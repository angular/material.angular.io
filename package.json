{
  "name": "material-angular-io",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "yarn build-themes && ng serve",
    "start:jit": "yarn build-themes && ng serve --aot=false",
    "start:prod": "yarn build-themes && ng serve --prod",
    "lint": "ng lint",
    "test": "yarn build-themes && ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "yarn build-themes && ng e2e",
    "build": "yarn build-themes && ng build",
    "build:content": "yarn upgrade @angular/components-examples",
    "build:highmem": "yarn build-themes && node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build",
    "build:sm": "ng build --prod --source-map",
    "build-themes": "bash ./tools/build-themes.sh",
    "prod-build": "yarn build-themes && ng build --prod",
    "postinstall": "webdriver-manager update --gecko false && yarn build-themes && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "publish-prod": "bash ./tools/deploy.sh stable prod",
    "publish-dev": "bash ./tools/deploy.sh",
    "publish-beta": "bash ./tools/deploy.sh stable beta"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.6",
    "@angular/cdk": "^9.2.0-next.0",
    "@angular/cdk-experimental": "^9.2.0-next.0",
    "@angular/common": "9.0.6",
    "@angular/compiler": "9.0.6",
    "@angular/components-examples": "angular/material2-docs-content#master",
    "@angular/core": "9.0.6",
    "@angular/forms": "9.0.6",
    "@angular/google-maps": "^9.2.0-next.0",
    "@angular/material": "^9.2.0-next.0",
    "@angular/material-experimental": "^9.2.0-next.0",
    "@angular/material-moment-adapter": "^9.2.0-next.0",
    "@angular/platform-browser": "9.0.6",
    "@angular/platform-browser-dynamic": "9.0.6",
    "@angular/router": "9.0.6",
    "@angular/youtube-player": "^9.2.0-next.0",
    "material-components-web": "^6.0.0-canary.265ecbad5.0",
    "moment": "^2.24.0",
    "rxjs": "^6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.6",
    "@angular/cli": "9.0.6",
    "@angular/compiler-cli": "9.0.6",
    "@angular/localize": "9.0.6",
    "@types/jasmine": "^3.5.9",
    "@types/node": "^12.12.30",
    "codelyzer": "^5.2.1",
    "firebase-tools": "^7.15.1",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "node-sass": "^4.13.1",
    "protractor": "^5.4.3",
    "ts-node": "8.6.2",
    "tslint": "^5.20.1",
    "typescript": "~3.7.5"
  }
}
